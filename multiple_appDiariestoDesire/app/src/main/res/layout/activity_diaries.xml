<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Diaries">

    <ImageView
        android:id="@+id/diariessection"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/diaries_section" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="93dp"
        android:layout_height="131dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.106"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.196"
        app:srcCompat="@drawable/creativejournal" />

    <ImageButton
        android:id="@+id/label1"
        android:layout_width="177dp"
        android:layout_height="197dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/diariessection"
        app:layout_constraintVertical_bias="0.273"
        app:srcCompat="@drawable/label_" />

    <TextView
        android:id="@+id/text1"
        android:layout_width="108dp"
        android:layout_height="23dp"
        android:fontFamily="@font/leckerli_one"
        android:text="Creative Journal"
        android:textColor="#121111"
        android:textColorHighlight="#020202"
        android:textColorHint="#0F0E0E"
        android:textColorLink="#100F0F"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/label1"
        app:layout_constraintEnd_toEndOf="@+id/label1"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="@+id/label1"
        app:layout_constraintTop_toTopOf="@+id/label1"
        app:layout_constraintVertical_bias="0.551" />

    <ImageView
        android:id="@+id/travel"
        android:layout_width="87dp"
        android:layout_height="115dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.766"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.178"
        app:srcCompat="@drawable/travel_journal" />

    <ImageButton
        android:id="@+id/label2"
        android:layout_width="177dp"
        android:layout_height="177dp"
        android:backgroundTint="#00FFFFFF"
        android:onClick="label2"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/label1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.273"
        app:srcCompat="@drawable/label_" />

    <TextView
        android:id="@+id/text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/leckerli_one"
        android:text="Travel Journal"
        android:textColor="#141313"
        android:textColorHighlight="#070707"
        android:textColorHint="#0D0C0C"
        android:textColorLink="#090909"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/label2"
        app:layout_constraintEnd_toEndOf="@+id/label2"
        app:layout_constraintHorizontal_bias="0.453"
        app:layout_constraintStart_toStartOf="@+id/label2"
        app:layout_constraintTop_toTopOf="@+id/label2"
        app:layout_constraintVertical_bias="0.539" />

    <ImageView
        android:id="@+id/art"
        android:layout_width="87dp"
        android:layout_height="115dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.123"
        app:layout_constraintStart_toStartOf="@+id/diariessection"
        app:layout_constraintTop_toTopOf="@+id/diariessection"
        app:srcCompat="@drawable/art_diary" />

    <ImageButton
        android:id="@+id/label3"
        android:layout_width="177dp"
        android:layout_height="177dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/diariessection"
        app:layout_constraintVertical_bias="0.619"
        app:srcCompat="@drawable/label_" />

    <TextView
        android:id="@+id/text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/leckerli_one"
        android:text="Art journal"
        android:textColor="#1B1A1A"
        android:textColorHighlight="#121111"
        android:textColorHint="#0F0E0E"
        android:textColorLink="#0B0B0B"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/label3"
        app:layout_constraintEnd_toEndOf="@+id/label3"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/label3"
        app:layout_constraintVertical_bias="0.55" />

    <ImageView
        android:id="@+id/journal"
        android:layout_width="105dp"
        android:layout_height="143dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/diariessection"
        app:layout_constraintHorizontal_bias="0.743"
        app:layout_constraintStart_toEndOf="@+id/art"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.476"
        app:srcCompat="@drawable/journal" />

    <ImageButton
        android:id="@+id/label4"
        android:layout_width="177dp"
        android:layout_height="177dp"
        android:backgroundTint="#01FFFFFF"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/label3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.613"
        app:srcCompat="@drawable/label_" />

    <TextView
        android:id="@+id/text4"
        android:layout_width="86dp"
        android:layout_height="17dp"
        android:fontFamily="@font/leckerli_one"
        android:text="Nature journal"
        android:textColor="#0F0E0E"
        android:textColorHighlight="#040404"
        android:textColorHint="#161515"
        android:textColorLink="#090909"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/label4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.439"
        app:layout_constraintStart_toStartOf="@+id/label4"
        app:layout_constraintTop_toTopOf="@+id/label4"
        app:layout_constraintVertical_bias="0.568" />

    <ImageView
        android:id="@+id/gratitude"
        android:layout_width="87dp"
        android:layout_height="115dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/diariessection"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.123"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.756"
        app:srcCompat="@drawable/gratitude" />

    <ImageButton
        android:id="@+id/label5"
        android:layout_width="177dp"
        android:layout_height="177dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.068"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.933"
        app:srcCompat="@drawable/label_" />

    <TextView
        android:id="@+id/text5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/leckerli_one"
        android:text="Gratitude journal"
        android:textColor="#211F1F"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/label5"
        app:layout_constraintEnd_toEndOf="@+id/label5"
        app:layout_constraintHorizontal_bias="0.407"
        app:layout_constraintStart_toStartOf="@+id/label5"
        app:layout_constraintTop_toTopOf="@+id/label5"
        app:layout_constraintVertical_bias="0.55" />

    <ImageView
        android:id="@+id/health"
        android:layout_width="87dp"
        android:layout_height="115dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.766"
        app:layout_constraintStart_toEndOf="@+id/gratitude"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.756"
        app:srcCompat="@drawable/health_diary" />

    <ImageButton
        android:id="@+id/label6"
        android:layout_width="181dp"
        android:layout_height="179dp"
        android:layout_marginBottom="32dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/diariessection"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/label_" />

    <TextView
        android:id="@+id/text6"
        android:layout_width="99dp"
        android:layout_height="19dp"
        android:fontFamily="@font/leckerli_one"
        android:text="Health Journal"
        android:textColor="#070707"
        android:textColorHighlight="#0B0A0A"
        android:textColorHint="#121212"
        android:textColorLink="#0D0C0C"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/label6"
        app:layout_constraintEnd_toEndOf="@+id/label6"
        app:layout_constraintHorizontal_bias="0.439"
        app:layout_constraintStart_toStartOf="@+id/label6"
        app:layout_constraintTop_toTopOf="@+id/label6"
        app:layout_constraintVertical_bias="0.531" />

    <Button
        android:id="@+id/thanksbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#715109"
        android:fontFamily="@font/leckerli_one"
        android:text="Thank you"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.529"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.945" />

</androidx.constraintlayout.widget.ConstraintLayout>